   "source": [
    "from py3d import Triangle,Vector3,Color\n",
    "t=Triangle()\n",
    "t.geometry.vertice=Vector3([\n",
    "    [4,0,0],\n",
    "    [0,1,0],\n",
    "    [0,-1,0]\n",
    "])\n",
    "t.geometry.color=Color(r=1)\n",
    "t.render()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from py3d import Transform\n",
    "t.transform = Transform.from_rpy([1,0,3])\n",
    "t.render()"
   ]
  }
